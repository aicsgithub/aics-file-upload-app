{
  "name": "file-upload-app",
  "version": "1.0.59",
  "build": {
    "appId": "org.aics.alleninstitute.fileupload",
    "files": [
      "dist/main/*",
      "dist/renderer/*"
    ],
    "linux": {
      "target": "AppImage"
    },
    "publish": {
      "provider": "s3",
      "bucket": "file-upload-app.allencell.org",
      "region": "us-west-2"
    },
    "win": {
      "target": "portable"
    }
  },
  "scripts": {
    "dev": "cross-env NODE_TLS_REJECT_UNAUTHORIZED='0' electron-webpack dev",
    "compile": "cross-env NODE_TLS_REJECT_UNAUTHORIZED='0' electron-webpack",
    "compile-cache": "cross-env ELECTRON_WEBPACK_APP_LIMS_HOST=\"stg-aics.corp.alleninstitute.org\" ELECTRON_WEBPACK_APP_LIMS_PORT=80 NODE_TLS_REJECT_UNAUTHORIZED='0' ELECTRON_WEBPACK_USE_CACHE=true yarn compile",
    "compile-stage": "cross-env ELECTRON_WEBPACK_APP_LIMS_HOST=\"stg-aics.corp.alleninstitute.org\" ELECTRON_WEBPACK_APP_LIMS_PORT=80 NODE_TLS_REJECT_UNAUTHORIZED='0' yarn compile",
    "compile-prod": "cross-env ELECTRON_WEBPACK_APP_LIMS_HOST=\"aics.corp.alleninstitute.org\" ELECTRON_WEBPACK_APP_LIMS_PORT=80 NODE_TLS_REJECT_UNAUTHORIZED='0' NODE_ENV=production yarn compile",
    "build-executable": "yarn compile && yarn electron-builder",
    "dist": "electron-builder -p always",
    "test": "cross-env TS_NODE_PROJECT=tsconfig.commonjs.json TS_NODE_FILES=true NODE_ENV=production mocha src/**/test/*.{ts,tsx}",
    "postinstall": "electron-builder install-app-deps",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "madge": "madge --warning --circular --ts-config tsconfig.base.json --webpack-config webpack/webpack.render.additions.js --extensions js,jsx,ts,tsx  src/"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:aicsgithub/aics-file-upload-app.git"
  },
  "keywords": [
    "electron",
    "react",
    "aics"
  ],
  "resolutions": {
    "@types/react": "16.9.2"
  },
  "author": "Lisa Harrylock",
  "license": "ISC",
  "dependencies": {
    "@aics/aics-react-labkey": "^4.6.3",
    "@ant-design/icons": "~1.1.0",
    "@types/amqplib": "^0.5.17",
    "amqplib": "^0.6.0",
    "antd": "3.26.0",
    "axios": "0.19.0",
    "chai-as-promised": "7.1.1",
    "core-js": "2.6.5",
    "electron-devtools-installer": "^3.1.1",
    "electron-store": "5.2.0",
    "electron-updater": "4.2.2",
    "humps": "2.0.1",
    "js-logger": "1.6.0",
    "jsdom": "14.0.0",
    "jsdom-global": "3.0.2",
    "lodash": "4.17.20",
    "moment": "2.24.0",
    "object-hash": "2.0.3",
    "react": "16.9.0",
    "react-data-grid": "6.1.0",
    "react-dom": "16.9.0",
    "redux-undo": "1.0.0-beta9-9-7",
    "reselect": "4.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "6.5.4",
    "source-map-support": "0.5.11",
    "ts-import-plugin": "1.5.5",
    "ts-node": "9.0.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/chai-as-promised": "7.1.2",
    "@types/classnames": "2.2.7",
    "@types/electron-devtools-installer": "2.2.0",
    "@types/electron-json-storage": "4.0.0",
    "@types/enzyme": "3.9.0",
    "@types/humps": "1.1.2",
    "@types/jsdom": "12.2.3",
    "@types/lodash": "4.14.123",
    "@types/mocha": "8.0.4",
    "@types/node": "12.12.47",
    "@types/object-hash": "1.3.3",
    "@types/react": "16.9.2",
    "@types/react-data-grid": "4.0.5",
    "@types/react-dom": "16.9.0",
    "@types/react-redux": "7.1.5",
    "@types/rimraf": "^3.0.0",
    "@types/semver": "6.0.0",
    "@types/sinon": "^9.0.5",
    "@types/sinon-chai": "^3.2.4",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "2.32.0",
    "@typescript-eslint/parser": "2.32.0",
    "chai": "4.2.0",
    "classnames": "2.2.6",
    "cross-env": "5.2.0",
    "css-loader": "2.1.1",
    "electron": "10.1.5",
    "electron-builder": "22.9.1",
    "electron-webpack": "2.6.2",
    "electron-webpack-ts": "3.1.1",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.11.2",
    "eslint": "7.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-no-only-tests": "2.4.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "4.0.0",
    "husky": ">=4",
    "less": "3.9.0",
    "less-loader": "4.1.0",
    "lint-staged": ">=10",
    "madge": "^3.9.2",
    "mocha": "8.2.1",
    "mock-css-modules": "2.0.0",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.6.0",
    "prettier": "2.0.5",
    "react-redux": "7.1.1",
    "redux": "4.0.4",
    "redux-logic": "2.1.1",
    "sinon": "^9.0.3",
    "sinon-chai": "^3.5.0",
    "style-loader": "0.23.1",
    "typescript": "3.7.2",
    "webpack": "4.28.4",
    "worker-loader": "2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "eslint --cache --fix"
  }
}
