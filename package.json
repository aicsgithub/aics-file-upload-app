{
  "name": "file-upload-app",
  "version": "1.0.12",
  "build": {
    "appId": "org.aics.alleninstitute.fileupload",
    "files": [
      "dist/main/*",
      "dist/renderer/*"
    ],
    "publish": {
      "provider": "s3",
      "bucket": "file-upload-app.allencell.org",
      "region": "us-west-2"
    },
    "win": {
      "target": "portable"
    }
  },
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "compile-cache": "cross-env ELECTRON_WEBPACK_APP_LIMS_HOST=\"stg-aics.corp.alleninstitute.org\" ELECTRON_WEBPACK_APP_LIMS_PORT=80 ELECTRON_WEBPACK_USE_CACHE=true yarn compile",
    "compile-stage": "cross-env ELECTRON_WEBPACK_APP_LIMS_HOST=\"stg-aics.corp.alleninstitute.org\" ELECTRON_WEBPACK_APP_LIMS_PORT=80 yarn compile",
    "compile-prod": "cross-env ELECTRON_WEBPACK_APP_LIMS_HOST=\"aics.corp.alleninstitute.org\" ELECTRON_WEBPACK_APP_LIMS_PORT=80 NODE_ENV=production yarn compile",
    "dist": "electron-builder -p always",
    "test": "cross-env TS_NODE_PROJECT=tsconfig.commonjs.json NODE_ENV=production mocha --opts mocha.opts src/**/test/*.{ts,tsx}",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:aicsgithub/aics-file-upload-app.git"
  },
  "keywords": [
    "electron",
    "react",
    "aics"
  ],
  "resolutions": {
    "@types/react": "16.8.17"
  },
  "author": "Lisa Harrylock",
  "license": "ISC",
  "dependencies": {
    "@aics/aics-react-labkey": "^4.1.6",
    "@aics/aicsfiles": "^2.0.2",
    "@aics/job-status-client": "^1.0.8",
    "@ant-design/icons": "~1.1.0",
    "antd": "3.20.1",
    "axios": "0.19.0",
    "core-js": "2.6.5",
    "electron-store": "3.2.0",
    "electron-updater": "4.0.6",
    "humps": "^2.0.1",
    "js-logger": "1.6.0",
    "jsdom": "14.0.0",
    "jsdom-global": "3.0.2",
    "lodash": "4.17.13",
    "mocha": "6.0.2",
    "react": "16.8.6",
    "react-data-grid": "5.0.4",
    "react-dom": "16.8.6",
    "redux-undo": "1.0.0-beta9-9-7",
    "reselect": "4.0.0",
    "source-map-support": "0.5.11",
    "ts-import-plugin": "1.5.5",
    "ts-node": "^8.2.0"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/chai": "4.1.7",
    "@types/classnames": "2.2.7",
    "@types/electron-json-storage": "4.0.0",
    "@types/enzyme": "3.9.0",
    "@types/humps": "^1.1.2",
    "@types/jsdom": "12.2.3",
    "@types/lodash": "4.14.123",
    "@types/mocha": "5.2.6",
    "@types/node": "11.11.3",
    "@types/react": "16.8.17",
    "@types/react-data-grid": "4.0.3",
    "@types/react-dom": "16.8.4",
    "@types/react-redux": "7.0.3",
    "@types/semver": "6.0.0",
    "@types/sinon": "7.0.10",
    "chai": "4.2.0",
    "classnames": "2.2.6",
    "cross-env": "5.2.0",
    "css-loader": "2.1.1",
    "electron": "5.0.3",
    "electron-builder": "20.42.0",
    "electron-webpack": "2.6.2",
    "electron-webpack-ts": "3.1.1",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.11.2",
    "less": "3.9.0",
    "less-loader": "4.1.0",
    "mock-css-modules": "1.0.0",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.6.0",
    "react-redux": "6.0.1",
    "redux": "4.0.1",
    "redux-logic": "2.1.1",
    "sinon": "7.2.7",
    "style-loader": "0.23.1",
    "tslint": "5.14.0",
    "tslint-no-unused-expression-chai": "0.1.4",
    "tslint-react": "3.6.0",
    "typescript": "3.3.3333",
    "webpack": "4.28.4"
  }
}
